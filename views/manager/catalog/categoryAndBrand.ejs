<!-- NOTE: CONTAING BOTH CATEGORY AND BRAND -->
<% include ../../navbar/admin %>

<br><br>
<div class="container">
  <div class="row">
    <div class="col s6 pull-s1">
      <div class="row">
        <div class="row">
          <ul class="collection with-header">
            <li class="collection-header">
              <h6>Categories</h6>
            </li>
            <% category.forEach((category) => {%>
            <li class="collection-item">
              <div>
                <%= category.category_name %>
              </div>
            </li>
            <%})%>
          </ul>
        </div>
      </div>
    </div><!-- main col -->

    <!-- brand actions -->
    <div class="col s6 push-s1">
      <div class="row">
        <ul class="collection with-header">
          <li class="collection-header">
            <h6>Brands</h6>
          </li>
          <% brand.forEach((brand) => {%>
          <li class="collection-item">
            <div>
              <%= brand.brand_name %>
            </div>
          </li>
          <%})%>
        </ul>
      </div>
    </div>
  </div>
</div>